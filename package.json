{
  "name": "react-flat",
  "author": "K3ira",
  "description": "Flat designed components made with and for ReactJS",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint ./components/**/*.jsx --color --quiet",
    "test": "mocha --compilers js:test/compiler.js --require test/setup.js test/**/*.spec.js",
    "build": "node ./build/build.js",
    "start": "npm run clean & npm run build"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "prop-types": "^15.5.10",
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "immutable": "^3.8.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "open": "0.0.5",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1"
  },
  "eslintConfig": {
    "extends": "./eslint/index.js"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "exclude": [
            "transform-exponentiation-operator"
          ],
          "debug": false
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "syntax-dynamic-import"
    ]
  }
}
