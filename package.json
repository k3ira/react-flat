{
  "name": "react-flat",
  "author": "K3ira",
  "description": "Flat designed components made with and for ReactJS",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint ./components/**/*.jsx --color --quiet",
    "build": "babel ./components --out-dir ./dist",
