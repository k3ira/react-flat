    "start": "cross-env NODE_ENV=development npm run clean & npm run build & npm run server"
  },
  "engines": {
    "node": "6.5.0"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "~6.5.1",
    "babel-cli": "~6.18.0",
    "babel-core": "~6.17.0",
    "babel-eslint": "~7.0.0",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.16.0",
    "babel-preset-react": "~6.16.0",
    "cross-env": "~3.1.3",
    "css-loader": "~0.25.0",
    "eslint": "~3.8.1",
    "eslint-plugin-babel": "~3.3.0",
    "eslint-plugin-react": "~6.4.1",
    "express": "~4.14.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "node-sass": "~3.10.1",
    "npm-run-all": "~3.1.1",
    "react": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-router": "~2.8.1",
    "sass-loader": "~4.0.2",
    "style-loader": "~0.13.1",
    "webpack": "~1.13.2",
    "webpack-dev-middleware": "~1.8.4",
    "webpack-hot-middleware": "~2.13.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
